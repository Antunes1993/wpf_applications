<Window x:Class="app_01.Windows.return_info_main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:app_01.Windows"
        mc:Ignorable="d"
        Title="return_info_main" Height="200" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" MinHeight="100"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <StackPanel Margin="10">
                <Label Content="Result" BorderBrush="Transparent"/>
                <TextBox Name="tbResult" IsEnabled="False" Width="150" Height="40"/>
            </StackPanel>
        </Grid>
        <Button Grid.Row="1" Name="btngetResult" Click="btngetResult_Click" Content="Get user Result" Width="100" Height="25"/>
    </Grid>
</Window>
